{
    "name": "Open with...",
    "description": "Open a website with...",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "scripting",
        "activeTab",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.medium.com/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}